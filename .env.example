# Airtable Configuration
AIRTABLE_API_KEY=your_api_key_here
AIRTABLE_BASE_ID=your_base_id_here
AIRTABLE_TABLE_NAME=Events

# Club API Configuration
CLUB_API_KEY=your_club_api_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/clubevents

# Hack Club Auth OAuth Configuration (auth.hackclub.com)
HACKCLUB_AUTH_CLIENT_ID=your_hackclub_auth_client_id_here
HACKCLUB_AUTH_CLIENT_SECRET=your_hackclub_auth_client_secret_here
HACKCLUB_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback


# Session & Encryption Configuration
# Generate SESSION_SECRET: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
SESSION_SECRET=your_session_secret_here_32_bytes_base64
# Generate TOKEN_ENC_KEY: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
TOKEN_ENC_KEY=your_token_encryption_key_here_32_bytes_base64
